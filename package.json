{
  "name": "@samhwang/resume-page",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": "^20.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "prepare": "pnpm run git-hooks:init && pnpm run panda:codegen",
    "git-hooks:init": "husky",
    "panda:codegen": "panda codegen",
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "ci": "biome ci ."
  },
  "dependencies": {
    "@fontsource/mulish": "^5.2.5",
    "@fontsource/roboto": "^5.2.5",
    "@fontsource/saira-extra-condensed": "^5.2.5",
    "@popperjs/core": "^2.11.8",
    "@react-hookz/web": "^25.1.0",
    "@tanstack/react-router": "^1.114.27",
    "@tanstack/router-devtools": "^1.114.27",
    "bootstrap": "^5.3.3",
    "devicon": "^2.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@pandacss/dev": "^0.53.3",
    "@tanstack/router-cli": "^1.114.27",
    "@tanstack/router-vite-plugin": "^1.114.27",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.13",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "@vitest/ui": "^3.0.9",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-prepush": "^3.0.2",
    "lint-staged": "^15.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^0.21.2",
    "vitest": "^3.0.9",
    "wrangler": "^4.4.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["@biomejs/biome", "esbuild", "sharp", "workerd"]
