    "bootstrap": "^5.3.2",
    "clsx": "^2.0.0",
    "devicon": "^2.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@mojojoejo/vite-plugin-purgecss": "^1.1.0",
