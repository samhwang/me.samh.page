    "@vitest/ui": "^1.1.0",
    "jsdom": "^23.0.1",
    "netlify-cli": "^17.10.1",
    "sass": "^1.69.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.0"
