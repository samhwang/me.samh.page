{
  "name": "@samhwang/resume-page",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "msw": {
    "workerDirectory": "public"
  },
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "prepare": "pnpm run git-hooks:init && pnpm run panda:codegen",
    "git-hooks:init": "husky",
    "panda:codegen": "panda codegen",
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "ci": "biome ci ."
  },
  "dependencies": {
    "@fontsource/mulish": "^5.2.6",
    "@fontsource/roboto": "^5.2.6",
    "@fontsource/saira-extra-condensed": "^5.2.6",
    "@react-hookz/web": "^25.1.1",
    "@tanstack/react-router": "^1.127.2",
    "@tanstack/react-router-devtools": "^1.127.2",
    "devicon": "^2.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@pandacss/dev": "^0.54.0",
    "@tanstack/router-cli": "^1.127.2",
    "@tanstack/router-plugin": "^1.127.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.16.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-prepush": "^3.0.2",
    "lint-staged": "^16.1.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "sharp",
      "workerd"
    ]
